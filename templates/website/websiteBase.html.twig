<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}CarFLOw{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {% block stylesheets %}
        {{ encore_entry_link_tags('website') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('website') }}
    {% endblock %}
</head>
<body>
    <div class="content">
        <nav class="navbar navbar-default navbar-custom">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{ path('app_view_website_get-home-page-view') }}">CarFLOw</a>
                </div>

                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav">
                        <li><a href="{{ path('app_view_website_get-reservation-page-view') }}">REZERWACJA</a></li>
                        <li><a href="{{ path('app_view_website_get-fleet-page-view') }}">FLOTA</a></li>
                        <li><a href="{{ path('app_view_website_get-branches-page-view') }}">ODDZIAŁY</a></li>
                        <li><a href="{{ path('app_view_website_get-faq-page-view') }}">FAQ</a></li>
                        <li><a href="{{ path('app_view_website_get-contact-page-view') }}">KONTAKT</a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="{{ path('app_view_website_get-customer-panel-page-view') }}">PANEL KLIENTA</a></li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">ADRIAN FLORCZAK <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">EDYTUJ PROFIL</a></li>
                                <li role="separator" class="divider"></li>
                                <li><a href="#">WYLOGUJ SIĘ</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        {% block websiteBody %}{% endblock %}
    </div>
    <div class="footer" style="background-color: #676767; color: white;">
        <div class="container-fluid">
            <div class="row" style="padding: 40px;">
                <div class="col-md-4">
                    MENU<hr>
                    <a href="{{ path('app_view_admin_dashboard_get-index') }}">STRONA GŁÓWNA</a><br>
                    <a href="{{ path('app_view_website_get-pages-by-slug-page-view', {slug: 'rodo'}) }}">RODO</a><br>
                    <a href="{{ path('app_view_website_get-pages-by-slug-page-view', {slug: 'polityka-prywatnosci'}) }}">POLITYKA PRYWATNOŚCI</a><br>
                    <a href="javascript:void(0)">PANEL OBSŁUGI</a><br>
                    <a href="{{ path('app_view_admin_dashboard_get-index') }}">PANEL ADMINISTRATORA</a><br>
                </div>
                <div class="col-md-4">
                    ODDZIAŁY<hr>
                </div>
                <div class="col-md-4">
                    DANE KONTAKTOWE<hr>
                </div>
            </div>
        </div>
        <div style="background-color: black; text-align: center; padding: 20px 0">
            <div class="container-fluid">
                CarFLOw<br>
                Wrocław 2023<br>
                Stworzone przez <a style="color: greenyellow;" href="https://adrianflorczak.pl" target="_blank">Adrian Florczak</a>
            </div>
        </div>
    </div>
</body>
</html>
